[run]
source = .
omit =
    */tests/*
    */venv/*
    setup.py
    */__pycache__/*
    */site-packages/*

branch = True

[report]
precision = 2
show_missing = True
skip_covered = False
skip_empty = True

exclude_lines =
    pragma: no cover
    def __repr__
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    def __str__
    except ImportError:
    except ModuleNotFoundError:

ignore_errors = True

[html]
directory = htmlcov
